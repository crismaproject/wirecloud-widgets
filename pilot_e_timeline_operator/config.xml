    <vCard:addr>
      <vCard:Work>
        <vCard:email>maria.egly@ait.ac.at</vCard:email>
      </vCard:Work>
    </vCard:addr>

    <wire:hasPlatformWiring>
      <wire:PlatformWiring rdf:ID="wiring">
        <wire:hasOutputEndpoint>
          <wire:OutputEndpoint rdf:ID="output_1">
            <dcterms:title>indicator_filter</dcterms:title>
