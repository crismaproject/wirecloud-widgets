  <wire:Operator rdf:about="http://wirecloud.com/test">
    <dcterms:title>pilot_e_timeline</dcterms:title>
    <wire:displayName>Pilot E Timeline</wire:displayName>
    <dcterms:description>An operator to parametrize the CRISMA IndicatorView widget with the indicators to be visualized.</dcterms:description>
    <dcterms:creator rdf:resource="http://creatoruri/"/>
    <usdl-core:versionInfo>0.1.0.1</usdl-core:versionInfo>
    <wire:hasImageUri rdf:resource="img/catalogue.png"/>
    <usdl-core:utilizedResource rdf:resource="js/main.js"/>
